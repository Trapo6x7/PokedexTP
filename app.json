{
  "expo": {
    "name": "trapokedex",
    "slug": "trapokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/trapokedexicon.png",
    "scheme": "trapokedex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/trapokedexhome.png",
        "backgroundColor": "#ff0000ff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.trapo.js.pokenative",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/pokedex.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/trapokedexicon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4559a9c0-720b-4696-9608-1ed0bfd89814"
      }
    },
    "owner": "trapo.js"
  }
}
